<h2>Create New Heroine</h2>

<%= render 'errors' if !!flash[:messages] %>

<%= form_for(@heroine) do |f| %>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :power_id %>
  <%= f.collection_select :power_id, @powers, :id, :name %>
  <%= f.submit %>
<% end %>


<hr>
<%= link_to("Back to List of All Heroines", heroines_path) %>
<%if flash[:messages]%>
    <%flash[:messages].each do |message|%>
    <p><%=message%></p>
    <%end%>
<%end%>
<%= form_for @heroine do |f| %>
    Name: <%= f.text_field :name %><br />
    Super Name : <%= f.text_field :super_name %><br />
    <%= f.collection_select :power_id, Power.order(:name),:id,:name%>
  <%= f.submit %>
<% end %>
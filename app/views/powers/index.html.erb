<h2>All Superpowers</h2>

<%= button_to "Create New Power", new_power_path, method: :get %>
<%= button_to "All Heroines", heroines_path, method: :get %>

<ul>
  <% @powers.each do |power| %>
    <li>
      <%= link_to power.name, power_path(power) %>
      <br>
      Description: <%= power.description %>
    </li>
    <br>
  <% end %>
</ul>
